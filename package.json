    },
    "scripts": {
        "start": "webpack-dev-server",
        "build": "webpack",
        "build:dist": "webpack --config webpack.config.dist.js",
        "cordova-deploy": "rm -rf www/* && npm run build:dist && cordova build android --release -- --packageType=bundle",
        "cordova-run": "npm run build:dist && cordova run android --release",
        "download-phaser-typedef": "download --out typings https://raw.githubusercontent.com/photonstorm/phaser/master/types/phaser.d.ts"
    },
    "dependencies": {
        "@types/socket.io-client": "^1.4.32",
        "cordova-android": "^8.1.0",
        "cordova-plugin-screen-orientation": "^3.0.1",
        "cordova-plugin-whitelist": "^1.3.3",
        "es6-promise-plugin": "^4.2.2",
        "phaser": "github:rekys/phaser#skip-frames",
        "socket.io-client": "^2.3.0",
        "turn-based-combat-framework": "^0.0.2"
    },
    "devDependencies": {
        "@types/node": "^12.12.27",
        "@types/webpack": "^4.41.6",
        "@types/webpack-env": "^1.15.1",
        "css-loader": "^3.4.2",
        "file-loader": "^4.3.0",
        "html-webpack-plugin": "^3.2.0",
        "style-loader": "^1.1.3",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "url-loader": "^2.3.0",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-screen-orientation": {}
        },
