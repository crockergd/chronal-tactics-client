{
    "compilerOptions": {
        "target": "ES2016",
        "module": "Preserve",
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": false,
        "noImplicitAny": false,
        "noImplicitOverride": false,
        "esModuleInterop": true,
        "removeComments": true,
        "outDir": "./www"
    },
    "include": [
        "src/**/*.ts",
        "typings/*.d.ts"
    ]
}